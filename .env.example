# Database Configuration
POSTGRES_URL=postgresql://user@localhost:5432/database

# Stripe Configuration
# STRIPE_SECRET_KEY=sk_test_your_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_secret_here

# Application Configuration
BASE_URL=http://localhost:3000

# Authentication Secret (generate a random 32+ character string)
AUTH_SECRET=your_random_secret_key_change_this_in_production_32chars_min

# Mapbox
NEXT_PUBLIC_MAPBOX_ACCESS_TOKEN=your_mapbox_token_here

# Feature Flags for Synoptics V2 Migration
# Set to 'true' to enable new architecture components

# Core Components (Fully migrated)
NEXT_PUBLIC_USE_NEW_PROPERTIES_PANEL=false
NEXT_PUBLIC_USE_NEW_LAYOUT_EDITOR=false

# Optional Components (Infrastructure ready)
NEXT_PUBLIC_USE_NEW_HIERARCHY_MANAGER=false
NEXT_PUBLIC_USE_NEW_IMPORT_DIALOG=false
NEXT_PUBLIC_USE_NEW_LOCATION_FILTER=false

# Percentage-based rollout (0-100) - overridden by specific flags above
NEXT_PUBLIC_NEW_ARCH_ROLLOUT=0
